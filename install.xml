<?xml version="1.0" encoding="utf-8" ?>
<plugin name="members_on_map">
	<title>Onine members on Google map</title>
	<summary>This plugin allows administrators to watch online users on Google Map</summary>
	<author>Intelliants LLC</author>
	<contributor>Intelliants LLC</contributor>
	<version>3.1.0</version>
	<date>2014-07-21</date>
	<compatibility>3.1.2</compatibility>
	<status>active</status>

	<phrases>
		<phrase category="admin" key="online_members_map">Member's Locations</phrase>
	</phrases>

	<hooks>
		<hook name="adminDashboardStatistics" type="php" page_type="admin" filename="plugins/members_on_map/includes/hook.adminDashboardStatistics.php"><![CDATA[]]></hook>
		<hook name="smartyDashboardContentRight" type="smarty" filename="plugins/members_on_map/templates/admin/map.tpl"><![CDATA[]]></hook>
	</hooks>
</plugin>